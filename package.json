{
  "name": "semantic-release-s3-upload",
  "version": "0.0.0-development",
  "description": "semantic-release plugin that uploads files to Amazon S3",
  "keywords": [
    "semantic-release",
    "publish",
    "s3",
    "aws"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/semantic-release-s3-upload",
  "bugs": "https://github.com/softwareventures/semantic-release-s3-upload",
  "repository": "github:softwareventures/semantic-release-s3-upload",
  "license": "ISC",
  "scripts": {
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "tslint --project . && ava"
  },
  "sideEffects": false,
  "dependencies": {
    "@softwareventures/array": "^0.31.0",
    "@softwareventures/chain": "^0.1.2",
    "@types/mime": "^2.0.1",
    "aws-sdk": "^2.553.0",
    "globby": "^11.0.0",
    "mime": "^2.4.4",
    "tslib": "1.11.1"
  },
  "devDependencies": {
    "@softwareventures/tsconfig": "2.2.0",
    "@softwareventures/tslint-rules": "0.8.1",
    "@types/semantic-release": "15.13.1",
    "ava": "3.5.1",
    "cz-conventional-changelog": "3.1.0",
    "semantic-release": "17.0.4",
    "ts-node": "8.8.1",
    "tslint": "6.1.0",
    "typescript": "3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "peerDependencies": {
    "semantic-release": "^15.13.27 || ^16.0.0 || ^17.0.0"
  }
}
