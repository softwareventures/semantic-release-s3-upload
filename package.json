{
  "private": true,
  "name": "@softwareventures/semantic-release-s3-upload",
  "version": "0.0.0-development",
  "description": "semantic-release plugin that uploads files to Amazon S3",
  "keywords": ["semantic-release", "publish", "s3", "aws"],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/semantic-release-s3-upload",
  "bugs": "https://github.com/softwareventures/semantic-release-s3-upload",
  "repository": "github:softwareventures/semantic-release-s3-upload",
  "license": "ISC",
  "scripts": {
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "tslint --project . && ava"
  },
  "dependencies": {
    "@softwareventures/array": "^0.16.1",
    "@softwareventures/chain": "^0.1.2",
    "aws-sdk": "^2.553.0",
    "globby": "^10.0.1",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@softwareventures/tsconfig": "2.0.0",
    "@softwareventures/tslint-rules": "0.7.1",
    "@types/semantic-release": "15.13.1",
    "ava": "2.4.0",
    "cz-conventional-changelog": "3.0.2",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "typescript": "3.6.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "peerDependencies": {
    "semantic-release": "^15.13.27"
  }
}
